*GPDSpecVersion: "1.0"
*Include: "StdNames.gpd"
*GPDFileVersion: "1.0"
*GPDFileName: "PrinterA8Plus.GPD"
*ModelName: "A8Plus"
*MasterUnits: PAIR(300, 300)
*PrinterType: SERIAL
*CodePage: 936
*Feature: Orientation
{
    *rcNameID: =ORIENTATION_DISPLAY
    *DefaultOption: PORTRAIT
    *Option: PORTRAIT
    {
        *rcNameID: =PORTRAIT_DISPLAY
    }
    *Option: LANDSCAPE_CC270
    {
        *rcNameID: =LANDSCAPE_DISPLAY
    }
}

*Feature: InputBin
{
    *rcNameID: =PAPER_SOURCE_DISPLAY
    *DefaultOption: TRACTOR
    *Option: TRACTOR
    {
        *rcNameID: =TRACTOR_DISPLAY
    }
    *Option: MANUAL
    {
        *rcNameID: =MANUAL_FEED_DISPLAY
    }
}



*Feature: Resolution
{
    *rcNameID: =RESOLUTION_DISPLAY
    *DefaultOption: Option1
    *Option: Option1
    {
        *Name: "300 x 300 " =DOTS_PER_INCH
        *DPI: PAIR(300, 300)
        *TextDPI: PAIR(300, 300)
        *EXTERN_GLOBAL: *StripBlanks: LIST(LEADING,TRAILING)
	*PinsPerPhysPass: 1
        *PinsPerLogPass: 24
        *SpotDiameter: 100
        *Command: CmdSendBlockData { *Cmd : "<1D>v0<00>" %l{NumOfDataBytes / 24} "<1800>"}
    }
}

*Feature: PaperSize
{
    *rcNameID: =PAPER_SIZE_DISPLAY
    *DefaultOption: Option2
    *Option: Option1
    {
		*Name: "58 x 210mm"
        *PageDimensions: PAIR(696, 2520)
        *PrintableArea: PAIR(576, 2520)
        *PrintableOrigin: PAIR(60, 0)
    }

    *Option: Option2
    {
		*Name: "58 x 297mm"
        *PageDimensions: PAIR(696, 3564)
        *PrintableArea: PAIR(576, 3564)
        *PrintableOrigin: PAIR(60, 0)
    }

    *Option: Option3
    {
		*Name: "58 x 3276mm"
        *PageDimensions: PAIR(696, 39312)
        *PrintableArea: PAIR(576, 39312)
        *PrintableOrigin: PAIR(60, 0)
    }

    *Option: CUSTOMSIZE
    {
        *rcNameID: =USER_DEFINED_SIZE_DISPLAY
        *MinSize: PAIR(480, 264)
        *MaxSize: PAIR(696, 49140)
        *MaxPrintableWidth: 696
        *MinLeftMargin: 60
         *CursorOrigin: PAIR(60,0)
        *CenterPrintable?: TRUE
    }
}

*Feature:BlackMark
{
	*FeatureType:PRINTER_PROPERTY
	*Name:"Paper"
	*DefaultOption:Continous
	*Option:Continous
	{
		*Name:"Continuous"
		*Command:CmdSelect
		{
			 *Order: PAGE_FINISH.3
				*Cmd:""
		}
	}
	*Option:Label
	{
		*Name:"Label"
		*Command:CmdSelect
		{
			 *Order: PAGE_FINISH.3
			*Cmd:"<1D><0C>"
		}
	}

}



